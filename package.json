{
  "name": "cleanstack-isomorphic",
  "description": "A simple isomorphic framework to build react apps upon",
  "version": "1.0.0",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/richardanaya/cleanstack-isomorphic.git"
  },
  "homepage": "https://github.com/richardanaya/cleanstack-isomorphic/",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "starter",
    "boilerplate",
    "template",
    "webpack",
    "koa"
  ],
  "main": "dist/server.js",
  "scripts": {
    "start": "node --harmony ./dist/server.js",
    "build": "node ./node_modules/webpack/bin/webpack.js --colors --display-error-details --config configs/webpack.client.js",
    "watch": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config configs/webpack.client-watch.js"
  },
  "dependencies": {
    "babel-polyfill": "6.3.14",
    "co": "^4.6.0",
    "co-chan": "0.0.3",
    "history": "1.17.0",
    "immstruct": "^2.0.0",
    "immutable": "^3.7.6",
    "react": "0.14.6",
    "react-cursor-mixin": "^1.1.0",
    "react-dom": "0.14.6",
    "react-mdl": "^1.1.0",
    "react-router": "1.0.3"
  },
  "devDependencies": {
    "babel": "6.3.26",
    "babel-core": "6.3.26",
    "babel-loader": "6.2.1",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-react-hmre": "1.0.1",
    "babel-preset-stage-0": "6.3.13",
    "concurrently": "1.0.0",
    "json-loader": "0.5.4",
    "wait-run": "1.1.0",
    "webpack": "1.12.9",
    "webpack-dev-server": "1.14.0"
  },
  "engines": {
    "node": ">=0.10.32"
  }
}
